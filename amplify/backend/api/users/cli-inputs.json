{
  "version": 1,
  "paths": {
    "/users": {
      "name": "/users",
      "lambdaFunction": "listUsersFunction",
      "permissions": {
        "setting": "private",
        "auth": [
          "read"
        ]
      }
    },
    "/user/{AccessToken}": {
      "name": "/user/{AccessToken}",
      "lambdaFunction": "getUserFunction",
      "permissions": {
        "setting": "open"
      }
    }
  }
}